<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTDMyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <properties resource="generatorConfig.properties"/>
    <classPathEntry location="D:\maven\maven3.5\repository3.5\mysql\mysql-connector-java\5.1.38"/>
    <context id="OracleTables" targetRuntime="MyBatis3">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <commentGenerator>
            <property name="suppressAllComments" value="false" />
            <property name="suppressDate" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="${mybatis.datasource.driver-class-name}"
                        connectionURL="${mybatis.datasource.url}"
                        userId="${mybatis.datasource.username}"
                        password="${mybatis.datasource.password}">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!-- Plain Ordinary Java Object -->
        <javaModelGenerator targetPackage="com.ct.mybatis.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- sql -->
        <sqlMapGenerator targetPackage="com.ct.mybatis.config"  targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        <!-- Data Access Object -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.ct.mybatis.dao"  targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        <!-- table+ -->

        <table schema="" tableName="s_user" domainObjectName="BasDictionary" delimitAllColumns="true"/>

    </context>
</generatorConfiguration>
